          "editorMode": "code",
          "format": "table",
          "rawQuery": true,
          "rawSql": "SELECT\r\n  date_bin(INTERVAL '5 seconds', \"time\", TIMESTAMP '1970-01-01T00:00:00Z') AS \"time\",\r\n\r\n  AVG(CASE WHEN \"core_num\" = 0 THEN \"percent_usage\" END) AS \"core 0\",\r\n  AVG(CASE WHEN \"core_num\" = 1 THEN \"percent_usage\" END) AS \"core 1\",\r\n  AVG(CASE WHEN \"core_num\" = 2 THEN \"percent_usage\" END) AS \"core 2\",\r\n  AVG(CASE WHEN \"core_num\" = 3 THEN \"percent_usage\" END) AS \"core 3\",\r\n  AVG(CASE WHEN \"core_num\" = 4 THEN \"percent_usage\" END) AS \"core 4\",\r\n  AVG(CASE WHEN \"core_num\" = 5 THEN \"percent_usage\" END) AS \"core 5\",\r\n  AVG(CASE WHEN \"core_num\" = 6 THEN \"percent_usage\" END) AS \"core 6\",\r\n  AVG(CASE WHEN \"core_num\" = 7 THEN \"percent_usage\" END) AS \"core 7\"\r\n\r\nFROM \"cpu\"\r\nWHERE\r\n  \"time\" >= $__timeFrom AND \"time\" <= $__timeTo\r\nGROUP BY 1\r\nORDER BY 1;",
